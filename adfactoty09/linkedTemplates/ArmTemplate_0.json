				"typeProperties": {
					"url": "[parameters('datalake_bronze_csv_dynamic_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('datalake_bronze_csv_dynamic_accountKey')]"
