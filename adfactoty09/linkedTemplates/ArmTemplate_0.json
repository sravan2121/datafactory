{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfactoty09"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureDataLake_csv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLake_csv'"
		},
		"BL_CSV_DEV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BL_CSV_DEV'"
		},
		"datalake_bronze_csv_dynamic_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_bronze_csv_dynamic'"
		},
		"linked_sftp_dev_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linked_sftp_dev'"
		},
		"AzureDataLake_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlazuredatabricksdev01.dfs.core.windows.net/"
		},
		"datalake_bronze_csv_dynamic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlazuredatabricksdev01.dfs.core.windows.net/"
		},
		"linked_sftp_dev_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "eu-central-1.sftpcloud.io"
		},
		"linked_sftp_dev_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "a9eb5d35fe96443c8ba85e9d36cc6223"
		},
		"AzureDataLakeStorage942_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/6e61c688-bf7a-4071-9186-02e3b1d11159/resourceGroups/rgdev01/providers/Microsoft.Storage/storageAccounts/dlazuredatabricksdev01"
		},
		"AzureDataLakeStorage942_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		}
